{
    "name": "proklung/annotatedparamresolverbundle",
    "description": "Symfony bundle for transform parameters of request to DTO.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gavrilov Fedy",
            "email": "fedor.gavrilov.75@mail.ru",
            "homepage": "https://github.com/ProklUng"
        }
    ],
    "support": {
        "issues": "https://github.com/ProklUng/AnnotatedParamResolverBundle/issues",
        "source": "https://github.com/ProklUng/AnnotatedParamResolverBundle"
    },
    "autoload": {
        "psr-4": {
