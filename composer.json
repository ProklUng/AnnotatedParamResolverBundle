        "symfony/validator": "^4.0 || ^5.0",
        "symfony/serializer": "^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.2 || ^8.0 || ^9.0",
        "mockery/mockery": "^1.4",
